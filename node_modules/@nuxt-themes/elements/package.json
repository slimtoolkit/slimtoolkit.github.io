{
  "version": "0.3.3",
  "name": "@nuxt-themes/elements",
  "type": "module",
  "main": "./nuxt.config.ts",
  "files": [
    "components",
    "app.config.ts",
    "nuxt.config.ts",
    "tokens.config.ts"
  ],
  "scripts": {
    "prepare": "nuxi prepare docs",
    "dev": "nuxi dev docs",
    "build": "nuxi build docs",
    "generate": "nuxi generate docs",
    "preview": "nuxi preview docs",
    "lint": "eslint .",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt-themes/tokens": "^1.4.2",
    "@vueuse/core": "^9.8.2"
  },
  "devDependencies": {
    "@nuxt-themes/docus": "^1.2.2",
    "@nuxt/content": "^2.3.0",
    "@nuxt/kit": "^3.0.0",
    "nuxt": "^3.0.0",
    "postcss": "^8.4.20",
    "release-it": "^15.5.1",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios",
        "@algolia/client-search"
      ]
    }
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm build"
      ]
    },
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
